# Never include any .env files anywhere
.env
**/.env
.env.*
env/
.env/

# Python cache files - don't rely on dots
**/__pycache__
**/*.pyc
**/*.pyo
**/*.pyd
**/*.sesskey

# Mac files - don't rely on leading dots
.DS_Store
**/.DS_Store

# Version control - be explicit
.git
.gitignore
.github
.dockerignore

# Other patterns
data/
dist/
build/
coverage/
*.log
Dockerfile
docker-compose.yml
docker-compose.*.yml
test/
tests/
tmp/
temp/